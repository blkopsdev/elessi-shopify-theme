{%- capture gecko_settings_css -%}
<style type="text/css">
{%- assign font_body = settings.font_body_text -%}
{%- assign font_titles = settings.font_titles_text -%}
{%- assign font_sub_title = settings.font_sub_title_text -%}
{%- for variant in font_body.variants -%}
    {{ variant | font_face }}
{%- endfor -%}
{%- if font_titles.family != font_body.family -%}
{{ font_titles | font_face }}
{%- endif -%}
{%- if font_sub_title.family != font_body.family and font_sub_title.family != font_titles.family -%}
{{ font_sub_title | font_face }}
{%- endif -%}
body{
    font-family: {{ font_body.family }}, {{ font_body.fallback_families }};
    font-style: normal;
    font-size: {{settings.size_body_text}}px;
    line-height: {{settings.body_line_height}};
    color: {{settings.color_body_text}};
    background-color: {{settings.body_bg}};
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: {{ font_titles.family }}, {{ font_titles.fallback_families }};
  font-weight: {{font_titles.weight}};
  font-style: {{font_titles.style}};
  font-size: {{settings.size_body_text}}px;
  line-height: {{settings.titles_line_height}};
}

h1,
h2,
h3,
h4,
h5,
h6 {
    color: {{settings.color_secondary}}
}

h1 {
    font-size: {{settings.size_text_h1}}px;
}

h2 {
    font-size: {{settings.size_text_h2}}px;
}

h3 {
    font-size: {{settings.size_text_h3}}px;
}

h4 {
    font-size: {{settings.size_text_h4}}px;
}

h5 {
    font-size: {{settings.size_text_h5}}px;
}

h6 {
    font-size: {{settings.size_text_h6}}px;
}
.single_variation_wrap .add_to_cart_button {
  padding: 0 10px;
  width:100%;
}
.shopify-payment-button,.checkout_divide {
    margin-top: 10px;
    max-width: 340px;
}
@media (min-width: 1024px) {
  .single_variation_wrap .add_to_cart_button {
    padding: 0 65px;
    width:auto;
  }
  .variations_form_qs .single_variation_wrap .add_to_cart_button {
    padding: 0 10px;
    width: 100%;
  }
}
@media (min-width: 736px) {
  .nasa-title .nasa-title-wrap span {font-size: {{settings.size_heading}}px;}
}
.btn_atc_full .shopify-payment-button, .btn_atc_full .checkout_divide { max-width: 100%}
.sub-title,.f__libre,.metaslider .flexslider .caption-wrap h4,blockquote, q {
  font-family: {{ font_sub_title.family }}, {{ font_sub_title.fallback_families }};
  font-weight: {{font_sub_title.weight}};
  font-style: {{font_sub_title.style}};
}
.nt_bg_lz.lazyload,.nt_bg_lz.lazyloading {
    background-color: {{settings.bg_lazyload}};
}
.nt-wrap-lazy:before {
    background-color: {{settings.color_lazyload}};
}
{%- if settings.body_bg != blank or settings.body_bg_image != blank -%}
body,body.nt_bg_lz {
    color: {{settings.color_body_text}};
    background-color: {{settings.body_bg}};
    {%- if settings.body_bg_image != blank -%}
    background-repeat: {{settings.body_bg_repeat}};
    background-size: {{settings.body_bg_size}};
    background-attachment: {{settings.body_bg_attachment}};
    background-position: {{settings.body_bg_position | replace: '_', ' ' }};
    {%- endif -%}
}
{%- endif -%}
{%- if settings.header_height != blank -%}
{%- assign header_height = settings.header_height | plus:0 -%}
{%- assign divided_pt = header_height | divided_by:2 -%}
{%- assign doune_pt = divided_pt | minus: 15 -%}
  .site_header .menu-section .nt_menu>li>a {
      height: {{settings.header_height}}px;
      line-height: {{settings.header_height}}px;
  }
  {%- comment -%}
  .site_header.design-header-3 .menu-section .nt_menu>li>a {
     {%- assign height_3 = header_height | plus: 5 | divided_by:2 -%}
      height: {{height_3}}px;
      line-height: {{height_3}}px;
  }
  {%- endcomment -%}
  .nathan_my-account .nav_dropdown,.nathan_my-account.nt_hover .nav_dropdown {
        padding: 0;
    box-shadow: none;
        background-color: transparent;
    padding-top: {{doune_pt}}px;
  }
  .nathan_my-account .nav_dropdown >div,.nathan_my-account.nt_hover .nav_dropdown >div {
        background-color: #fff;
    padding: 15px;
      -webkit-box-shadow: 0px 0px 35px -10px rgba(0, 0, 0, 0.25);
    box-shadow: 0px 0px 35px -10px rgba(0, 0, 0, 0.25);
  }
  .design-header-3 .header_wrapper {height: 100px;min-height: 100px}
 .fixed_height {height: {{settings.header_height}}px}
  .design-header-3 + .fixed_height {height: {{settings.header_height | plus:height_3 |plus:1}}px}
{%- endif -%}
{%- if settings.header_sticky_height != blank -%}
   {%- assign header_height = settings.header_sticky_height | plus:0 -%}
   {%- assign divided_pt = header_height | divided_by:2 -%}
   {%- assign doune_pt = divided_pt | minus: 15 -%}
   .live_stuck .menu-section .nt_menu > li > a {
    height: {{settings.header_sticky_height}}px;
    line-height: {{settings.header_sticky_height}}px;
}
  {%- comment -%}
  .live_stuck.design-header-3 .menu-section .nt_menu>li>a {
     {%- assign height_3 = header_height | plus: 5 | divided_by:2 -%}
      height: {{height_3}}px;
      line-height: {{height_3}}px;
  }
  {%- endcomment -%}
   .live_stuck .nathan_my-account .nav_dropdown,.live_stuck .nathan_my-account.nt_hover .nav_dropdown {
    padding-top: {{doune_pt}}px;
  }
   .live_stuck.design-header-3 .header_wrapper {
    min-height: {{settings.header_sticky_height}}px;
 }
   .live_stuck + .fixed_height {height: {{settings.header_sticky_height}}px}
   .design-header-3.live_stuck + .fixed_height {height: {{settings.header_height | plus:height_3 |plus:1}}px}

{%- endif -%}
{%- if settings.header_banner_bg != blank or settings.header_banner_bg_image != blank -%}
.announcement_bar {
    background-color: {{settings.header_banner_bg}};
    {%- if settings.header_banner_bg_image != blank -%}
    background-repeat: {{settings.header_banner_bg_repeat}};
    background-size: {{settings.header_banner_bg_size}};
    background-attachment: {{settings.header_banner_bg_attachment}};
    background-position: {{settings.header_banner_bg_position | replace: '_', ' ' }};
    {%- endif -%}
}
{%- endif -%}
{%- if settings.top_bar_bg != blank or settings.top_bar_bg_image != blank -%}
.header_top {
    background-color: {{settings.top_bar_bg}};
    {%- if settings.top_bar_bg_image != blank -%}
    background-repeat: {{settings.top_bar_bg_repeat}};
    background-size: {{settings.top_bar_bg_size}};
    background-attachment: {{settings.top_bar_bg_attachment}};
    background-position: {{settings.top_bar_bg_position | replace: '_', ' ' }};
    {%- endif -%}
}
{%- endif -%}
{%- if settings.header_bg != blank or settings.header_bg_image != blank -%}
.site_header {
    background-color: {{settings.header_bg}};
    {%- if settings.header_bg_image != blank -%}
    background-repeat: {{settings.header_bg_repeat}};
    background-size: {{settings.header_bg_size}};
    background-attachment: {{settings.header_bg_attachment}};
    background-position: {{settings.header_bg_position | replace: '_', ' ' }};
    {%- endif -%}
}
.header_transparent .site_header{
    {%- assign over_header_bg = settings.over_header_bg | divided_by: 100.0 -%}
    background-color: {{ settings.header_bg | color_modify: 'alpha', over_header_bg }};
}
  {%- if template.name != 'index' -%}
  body:not(.nt_has_img) .header_section{background-color: {{settings.header_bg}}}
  {%- endif -%}
{%- else -%}
    .header_transparent .site_header,.header_transparent .fixed_height{
    background-color: transparent;
   }
{%- endif -%}
{%- if settings.bg_header_sticky != blank -%}
   {%- assign over_header_bg_sticky = settings.over_header_bg_sticky | divided_by: 100.0 -%}
   .site_header.live_stuck,
   .header_transparent .site_header.live_stuck{
      background-color: {{ settings.bg_header_sticky | color_modify: 'alpha', over_header_bg_sticky }};
  }
{%- else -%}
   .header_transparent .site_header.live_stuck{
    background-color: #fff;
   }
{%- endif -%}
{%- if settings.disable_box_shadow -%}.site_header.live_stuck{box-shadow: none}{%- endif -%}
.footer-2.color-scheme-light .footer__bot >.container:before{display: none}
{%- assign over_header = settings.header_overlay | divided_by: 100.0 -%}
{%- assign over_footer = settings.footer_overlay | divided_by: 100.0 -%}
{%- assign top_bar_overlay = settings.top_bar_overlay | divided_by: 100.0 -%}
{%- assign header_banner_overlay = settings.header_banner_overlay | divided_by: 100.0 -%}
{%- assign body_overlay = settings.body_overlay | divided_by: 100.0 -%}
.announcement_bar.nt_bg_lz {box-shadow: inset 0 0 0 2000px {{ '#000000' | color_modify: 'alpha', header_banner_overlay }} }
.header_top.nt_bg_lz {box-shadow: inset 0 0 0 2000px {{ '#000000' | color_modify: 'alpha', top_bar_overlay }} }
.site_header.nt_bg_lz {box-shadow: inset 0 0 0 2000px {{ '#000000' | color_modify: 'alpha', over_header }} }
.footer__top.nt_bg_lz {box-shadow: inset 0 0 0 2000px {{ '#000000' | color_modify: 'alpha', over_footer }} }
 body.nt_bg_lz {box-shadow: inset 0 0 0 5000px {{ '#000000' | color_modify: 'alpha', body_overlay }} }
{%- assign _per_width = settings.logo_width -%}  
.nt-branding {
    width: {{_per_width}}%;
}  
.nt-branding img {
    max-width: {{ settings.logo_max_width | escape | default: '196' | remove: 'px' | append: 'px' }};
    max-height: {{ settings.logo_max_height | escape | default: '95' | remove: 'px' | append: 'px' }};
} 
.live_stuck .nt-branding img {
    max-width: {{ settings.logo_max_width_sticky | escape | default: '196' | remove: 'px' | append: 'px' }};
    max-height: {{ settings.logo_max_height_sticky | escape | default: '95' | remove: 'px' | append: 'px' }};
}
.header_transparent .nav_section_wrap {border-top-color: rgba(236, 236, 236, 0.3)}   
{%- if settings.header_design == '1' -%}
  {%- assign half_per_width = 100 | minus: _per_width | times: 1.0 | divided_by: 2 -%}
  .site_header .text_header,.site_header .menu-section {
      width: {{half_per_width}}%;
  }  
  .site_header .nathan-action {
      width: {{half_per_width}}%;
        white-space: nowrap;
  }
{%- elsif settings.header_design == '2' -%}
   .nathan-action {
    width: 247px;
     white-space: nowrap;
   }
{%- else -%}
  {%- assign half_per_width = 100 | minus: _per_width | times: 1.0 | divided_by: 2 -%}
  .design-header-3.site_header .text_header {
      width: {{half_per_width}}%;
   }
   .nathan-action {
    width: 247px;
     white-space: nowrap;
   }
   .design-header-3.site_header .nathan-action {
      width: {{half_per_width}}%;
        white-space: nowrap;
  }  
{%- endif -%}
{%- if settings.use_menu_color -%}
  .color-scheme-light .menu-section .nt_menu>li>a,
  .color-scheme-dark .menu-section .nt_menu>li>a,
  .color-scheme-light .menu-section .nt_menu>li.has-children>a:after,
  .color-scheme-dark .menu-section .nt_menu>li.has-children>a:after {
      color: {{settings.menu_color}};
  }
  .color-scheme-light.live_stuck .menu-section .nt_menu>li>a,
  .color-scheme-dark.live_stuck .menu-section .nt_menu>li>a,
  .color-scheme-light.live_stuck .menu-section .nt_menu>li.has-children>a:after,
  .color-scheme-dark.live_stuck .menu-section .nt_menu>li.has-children>a:after {
      color: {{settings.menu_color_sticky}};
  }
  .color-scheme-light .menu-section .nt_menu>li>a:hover,
  .color-scheme-dark .menu-section .nt_menu>li>a:hover,
  .color-scheme-light .menu-section .nt_menu>li>a:focus,
  .color-scheme-dark .menu-section .nt_menu>li>a:focus{
      color: {{ settings.menu_color | color_modify: 'alpha', 0.8 }};
  }
  .color-scheme-light.live_stuck .menu-section .nt_menu>li>a:hover,
  .color-scheme-dark.live_stuck .menu-section .nt_menu>li>a:hover,
  .color-scheme-light.live_stuck .menu-section .nt_menu>li>a:focus,
  .color-scheme-dark.live_stuck .menu-section .nt_menu>li>a:focus {
      color: {{ settings.menu_color_sticky | color_modify: 'alpha', 0.8 }};
  }
{%- endif -%}

{%- if settings.use_wishlist -%}{%- include 'nitro-wishlist' -%}{%- endif -%}
{%- if section.blocks.size > 0 -%}
   <nav role="navigation" class="text-{{settings.nathan_menu_align}} {{section.settings.hover}}">
      <ul id="nt_main_menu" class="nt_menu clearfix">
         {%- for block in section.blocks -%}
            {%- case block.type -%}
               {%- when 'shop' -%}
                {%- if block.settings.hidden_block == false -%}
                  {%- assign label = block.settings.label -%}
                  {%- assign icon = block.settings.icon -%}
                  {%- assign label_dow = label | downcase -%}
                  {%- assign link_list = block.settings.menu -%}
                  <li id="menu-item-{{block.id}}" class="type_shop fullwidth menu-item menu-item-hover has-children nt-has-children  menu-item-{{block.id}}{%- if label_dow == 'sale' or label_dow == 'sale!' %} sale{%- endif -%}">
                     <a href="{{block.settings.button_link | default:'/'}}">{%- if icon != blank -%}<i class="fa fa-{{icon}}"></i>{%- endif -%}{{block.settings.title}}{%- if label != blank and block.settings.color != blank -%}<span class="menu-item-tag menu-item-tag-{{ label | handleize}}">{{label}}</span>{%- endif -%}</a>
                     {%- if link_list != blank -%}

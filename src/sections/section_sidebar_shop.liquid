			   	      {%- if collections.size > 0 -%}
				   	      {%- for category in collections -%}
				   	         {%- if category.products.size > 0 -%}
					               <li class="cat-item{%- if collection.handle == category.handle %} current-cat{%- endif -%}"><a href="{{ category.url }}">{{category.title}}</a></li>
					            {%- endif -%}
				            {%- endfor -%}
			            {%- endif -%}
			        {%- else -%}
			            {%- assign top_link_list = block.settings.top_categories_link_list -%}
			            {%- for link in linklists[top_link_list].links -%}
			               {%- if link.type == 'collection_link' or link.type == 'catalog_link' -%}
			                  {%- assign child_list_handle = link.title | handleize -%}
			                  {%- if linklists[child_list_handle].links != blank -%}
		                       <li class="pr cat-item cat-parent{%- if link.active %} current-cat{%- endif -%}">
		                       	  <a href="{{ link.url }}">{{link.title}}</a>
		                       	  <ul class="sub-menu children">
		                       	  	   {%- for child_link in linklists[child_list_handle].links -%}
		                       	  	      {%- if child_link.type == 'collection_link' or child_link.type == 'catalog_link' -%}
		                       	  	      <li class="cat-item{%- if child_link.active %} current-cat{%- endif -%}">
		                       	  	      	<a href="{{ child_link.url }}">{{child_link.title}}</a>
		                       	  	      </li>
		                       	  	      {%- endif -%}
		                       	  	   {%- endfor -%}
		                       	  </ul>
		                       	</li>
		                     {%- elsif link.links != blank -%}
		                          <li class="pr menu_nested cat-item{%- if link.active %} current-cat{%- endif -%}">
			                       	  <a href="{{ link.url }}">{{link.title}}</a>
			                       	  <ul class="sub-menu">
			                       	  	   {%- for child_link in link.links -%}
			                       	  	      {%- if child_link.type == 'collection_link' or child_link.type == 'catalog_link' -%}
			                       	  	      <li class="cat-item{%- if child_link.active %} current-cat{%- endif -%}">
			                       	  	      	<a href="{{ child_link.url }}">{{child_link.title}}</a>
			                       	  	      </li>
			                       	  	      {%- endif -%}
			                       	  	   {%- endfor -%}
			                       	  </ul>
			                       	</li>
		                     {%- else -%}
		                       <li class="cat-item{%- if link.active %} current-cat{%- endif -%}"><a href="{{ link.url }}">{{link.title}}</a></li>
		                     {%- endif -%}
			               {%- endif -%}
			            {%- endfor -%}
			      {%- endcase -%}
			   </ul>
		      </aside>
		   {%- when 'collection' -%}
		    {%- assign image_size = '80x' -%}
			  <aside class="widget widget_products mb__30 clearfix">
			      <h4 class="pr tu widget-title fwsb fs__16 mb__25">{{ block.settings.title }}</h4>
			      {%- if block.settings.featured_product_list_collection == blank -%}
			         <p class="mg__0 mb__30">{{ 'homepage.onboarding.no_content' | t }}</p>
			      {%- else -%}
			      <ul class="product_list_widget">
				      {%- for product in collections[block.settings.featured_product_list_collection].products limit:block.settings.featured_product_limit_all -%}
				         {%- assign on_sale = false -%}
				         {%- if product.compare_at_price_min > product.price_min -%}
				           {%- assign on_sale = true -%}
				         {%- endif -%}
				         {%- assign c = block.settings.featured_product_list_collection -%}
				         <li>
				            <a href="{{product.url | within: collection}}" title="{{ product.title }}">
				               {%- assign image_0 = product.images[0] -%}
				            {%- if image_0 != blank -%}
				            <img src="{{ image_0 | img_url: image_size }}" srcset="{{ image_0 | img_url: image_size }} 1x, {{ image_0 | img_url: image_size, scale: 2 }} 2x" class="w__100 attachment-shop_catalog size-shop_catalog sp-post-image" alt="{{image_0.alt | escape }}">    
				            {%- endif -%}
				            </a>
				            <div class="pr_w_infors">
				               <div class="product-title"><a href="{{product.url | within: collection}}">{{ product.title }}</a></div>
				               <span class="price db mt__5">
				               {%- if product.price_varies -%}
				               {{ product.price_min | money }}â€“{{ product.price_max | money }}
				               {%- elsif on_sale -%}<ins>{{ product.price | money }}</ins> <del>{{ product.compare_at_price  | money }}</del>
				               {%- else -%}
				               {{ product.price | money }}
				               {%- endif -%}
				               </span>
				            </div>
				         </li>
				      {%- endfor -%}
				    </ul>
			      {%- endif -%}
			  </aside>
		   {%- when 'shipping' -%}
		      <aside id="jas_widget-{{block.id}}" class="widget widget_shipping mb__30">
				     <h4 class="pr tu widget-title fwsb fs__16 mb__25">{{ block.settings.title }}</h4>
				     <div class="row">
				     {%- for i in (1..6) -%}
                  {%- assign shipping = 'shipping_' | append:i -%}
                  {%- assign shipping_id = block.settings[shipping] -%}
                  {%- if shipping_id != blank -%}
                     {%- assign shipping = shipping_id | replace: ' ,', ',' | replace: ', ', ',' | split: ',' -%}
                        <div class="col-12 service-block tl{%- if forloop.first == false %} mt__30{%- endif -%}">
                        	{%- if shipping[0] != blank -%}<div class="service-icon medium cp"><i class="{%- if shipping[0] contains 'pe-' -%}{{shipping[0]}}{%- else -%}fa fa-{{shipping[0]}}{%- endif -%}"></i></div>{%- endif -%}
                           <div class="service-text">
                          	<h3 class="title text-c fwb fs__20 mb__5">{{shipping[1]}}</h3>
                             <p class="mg__0">{{shipping[2]}}</p>
                           </div>
                        </div>
                  {%- endif -%}
               {%- endfor -%}
             </div>
				</aside>
		   {%- when 'html' -%}
		      <aside id="jas_widget-{{block.id}}" class="widget mb__30">
				     <h4 class="pr tu widget-title fwsb fs__16 mb__25">{{ block.settings.title }}</h4>
				     {%- if block.settings.code != blank -%}{{block.settings.code}}{%- endif -%}
				</aside>
		   {%- when 'text' -%}
		      <aside id="jas_widget-{{block.id}}" class="widget mb__30">
				     <h4 class="pr tu widget-title fwsb fs__16 mb__25">{{ block.settings.title }}</h4>
				     {%- if block.settings.text != blank -%}{{block.settings.text}}{%- endif -%}
				</aside>
	   {%- endcase -%}

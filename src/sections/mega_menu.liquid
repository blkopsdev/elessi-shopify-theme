                           <div class="container">
                              <div class="row">
                                 <div class="{%- if block.settings.show_pr -%}col-9{%- else -%}col-12{%- endif -%}">
                                    <div class="row">
                                       {%- assign child_index = 0 -%}
                                       {%- assign no_index = 0 -%}
                                       {%- for link in linklists[link_list].links -%}
                                           {%- assign child_list_handle = link.title | handleize -%}
                                          {%- if linklists[child_list_handle].links != blank or link.links != blank -%}
                                          {%- assign child_index = child_index | plus:1 -%}
                                          {%- else -%}
                                          {%- assign no_index = no_index | plus:1 -%}
                                          {%- endif -%}
                                       {%- endfor -%}
                                       {%- if child_index > 5 %}
                                         {%- assign link_list_size = 6 -%}
                                       {%- elsif no_index > 6 -%}
                                         {%- assign link_list_size = child_index |plus:2 -%}
                                       {%- elsif no_index > 0 -%}
                                         {%- assign link_list_size = child_index |plus:1 -%}
                                       {%- else -%}
                                         {%- assign link_list_size = child_index -%}
                                       {%- endif -%}
                                       {%- if link_list_size == 5 -%}{%- assign class = 'lg-15' -%}
                                       {%- elsif link_list_size > 5 -%}{%- assign class = '2' -%}
                                       {%- else -%}{%- assign class = 12 | divided_by:link_list_size -%}
                                       {%- endif -%}
                                       {%- assign index = 0 -%}
                                       {%- for link in linklists[link_list].links limit:6 -%}
                                          {%- assign child_list_handle = link.title | handleize -%}
                                             {%- if linklists[child_list_handle].links != blank -%}
                                             {%- assign index = index |plus:1 -%}
                                             <div class="col-{{class}}">
                                                <ul class="sub-menu mega-menu">
                                                   <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                      <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                      <ul class="sub-column">
                                                         {%- for thirdlink in linklists[child_list_handle].links -%}
                                                            {%- assign icon_third  = thirdlink.title |  split: '[icon_' | last  | split: ']' | first -%}
                                                            {%- assign label_third  = thirdlink.title |  split: '[label_' | last  | split: ']' | first -%}
                                                            <li class="menu-item"><a href="{{ thirdlink.url }}">{%- if thirdlink.title contains '[icon_' -%}<i class="fa fa-{{icon_third}}"></i>{%- endif -%}{{ thirdlink.title | split: '[' | first }}{%- if thirdlink.title contains '[label_' -%}<span class="nt-menu-label nt-menu-label-{{label_third | handleize}}">{{label_third}}</span>{%- endif -%}</a></li>
                                                         {%- endfor -%}
                                                      </ul>
                                                   </li>
                                                </ul>
                                             </div>
                                             {%- elsif link.links != blank -%}
                                             {%- assign index = index |plus:1 -%}
                                             <div class="col-{{class}}">
                                                <ul class="sub-menu mega-menu">
                                                   <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                      <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                      <ul class="sub-column">
                                                         {%- for thirdlink in link.links -%}
                                                            {%- assign icon_third  = thirdlink.title |  split: '[icon_' | last  | split: ']' | first -%}
                                                            {%- assign label_third  = thirdlink.title |  split: '[label_' | last  | split: ']' | first -%}
                                                            <li class="menu-item"><a href="{{ thirdlink.url }}">{%- if thirdlink.title contains '[icon_' -%}<i class="fa fa-{{icon_third}}"></i>{%- endif -%}{{ thirdlink.title | split: '[' | first }}{%- if thirdlink.title contains '[label_' -%}<span class="nt-menu-label nt-menu-label-{{label_third | handleize}}">{{label_third}}</span>{%- endif -%}</a></li>
                                                         {%- endfor -%}
                                                      </ul>
                                                   </li>
                                                </ul>
                                             </div>
                                             {%- endif -%}
                                          {%- if index == 6 -%}{%- break -%}{%- endif -%}
                                       {%- endfor -%}
                                       {%- if child_index < 6 and no_index > 0 -%}
                                          <div class="col-{{class}}">
                                             <ul class="sub-menu mega-menu">
                                                {%- assign index_1 = 0 -%}
                                                {%- for link in linklists[link_list].links -%}
                                                   {%- assign child_list_handle = link.title | handleize -%}
                                                   {%- if linklists[child_list_handle].links == blank and link.links == blank -%}
                                                   <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                      <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                   </li>
                                                   {%- assign index_1 = index_1 |plus:1 -%}
                                                   {%- endif -%}
                                                   {% assign forindex = forloop.index -%}
                                                   {%- if index_1 == 6 -%}{%- break -%}{%- endif -%}
                                                {%- endfor -%}
                                             </ul>
                                          </div>
                                          {%- if no_index > 6 and child_index < 5 -%}
                                             <div class="col-{{class}}">
                                                <ul class="sub-menu mega-menu">
                                                   {%- assign index_2 = 0 -%}
                                                   {%- for link in linklists[link_list].links offset:forindex -%}
                                                      {%- assign child_list_handle = link.title | handleize -%}
                                                      {%- if linklists[child_list_handle].links == blank and link.links == blank -%}
                                                      <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                         <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                      </li>
                                                      {%- assign index_2 = index_2 |plus:1 -%}
                                                      {%- endif -%}
                                                      {%- if index_2 == 6 -%}{%- break -%}{%- endif -%}
                                                   {%- endfor -%}
                                                </ul>
                                             </div>
                                          {%- endif -%}
                                       {%- endif -%}
                                    </div>
                                    {%- if block.settings.show_ship -%}
                                    <div class="row mt__20 pt__20 nav_border_top">
                                       {%- assign style = block.settings.style -%}
                                       {%- for i in (1..3) -%}
                                          {%- assign shipping_heading = 'shipping_heading_' | append:i -%}
                                          {%- assign shipping_subtex = 'shipping_subtext_' | append:i -%}
                                          {%- assign shipping_icon = 'shipping_icon_' | append:i -%}
                                          {%- assign shipping_heading_id = block.settings[shipping_heading] -%}
                                          {%- assign shipping_subtex_id = block.settings[shipping_subtex] -%}
                                          {%- assign shipping_icon_id = block.settings[shipping_icon] -%}
                                          {%- if shipping_heading_id != blank and shipping_subtex_id != blank -%}
                                             <div class="spb_animate col-4">
                                                <div class="service-block {{block.settings.align}} service-style-{{style}}">
                                                   {%- if shipping_icon_id != blank -%}<div class="service-icon {{block.settings.icon_size}}" style="color: {{block.settings.icon_color}}"><i class="{%- if shipping_icon_id contains 'fa' %}fa {% endif -%}{{shipping_icon_id }}"></i></div>{%- endif -%}
                                                   <div class="service-text">
                                                      <h3 class="title text-c fwb fs__20 mb__5" style="color: {{block.settings.title_color}}">{{ shipping_heading_id }}</h3>
                                                      <p class="mg__0" style="color: {{block.settings.text_color}}">{{ shipping_subtex_id }}</p>
                                                   </div>
                                                </div>
                                             </div>
                                          {%- endif -%}
                                       {%- endfor -%}
                                    </div>
                                    {%- endif -%}
                                 </div>
                                 {%- if block.settings.show_pr -%}
                                 <div class="col-3 nav_border_left_none">
                                        {%- if block.settings.conditions_setting == '1' -%}
                                          {%- if block.settings.id_collection == blank -%}
                                             {%- assign collection = collections["all"] -%}
                                          {%- else -%}
                                             {%- assign collection = collections[block.settings.id_collection] -%}
                                          {%- endif -%}
                                          {%- if collection.products.size > 0 -%}
                                            {%- assign show_vendor = block.settings.show_vendor -%}
                                            {%- assign settings_pr_img_size = block.settings.use_eq_height -%}
                                            {%- assign img_size = block.settings.image_size -%}
                                            {%- assign section_settings_countdown = block.settings.sale_countdown -%}
                                            {%- assign countdown_timer_style = block.settings.countdown_style -%}
                                             {%- assign class_md = '12' -%}
                                             {%- assign class_xs = '12' -%}
                                             {%- assign many_products = block.settings.many_products -%}
                                                <div class="products row nt-products-holder nt-carousel {{block.settings.image_size}} {{block.settings.image_ratio}} position_{{block.settings.image_position}}{%- if settings_pr_img_size %} equal-nt{%- endif -%}" data-slick='{"slidesToShow": 1,"slidesToScroll": 1,"arrows":false,"dots": true,"rtl": {{ settings.use_rtl }}}'>
                                                   {%- for product in collection.products -%}
                                                      {%- include 'nt_product_loop' -%}
                                                      {%- if forloop.index == many_products -%}{%- break -%}{%- endif -%}
                                                   {%- endfor -%}
                                                </div>
                                          {%- endif -%}
                                       {%- else -%}
                                          {%- assign check_product_id = '' -%}
                                            {%- assign show_vendor = block.settings.show_vendor -%}
                                            {%- assign settings_pr_img_size = block.settings.use_eq_height -%}
                                            {%- assign img_size = block.settings.image_size -%}
                                            {%- assign section_settings_countdown = block.settings.sale_countdown -%}
                                            {%- assign countdown_timer_style = block.settings.countdown_style -%}
                                          {%- assign class_md = '12' -%}
                                          {%- assign class_xs = '12' -%}
                                             {%- capture html_pr -%}
                                                {%- for i in (1..5) -%}
                                                   {%- assign product_id = 'product_id_' | append:i -%}
                                                   {%- assign block_product_id = block.settings[product_id] -%}
                                                   {%- if block_product_id != blank -%}
                                                      {% assign product = all_products[block_product_id] %}
                                                      {%- unless check_product_id contains product.id -%}
                                                         {%- include 'nt_product_loop' -%}
                                                      {%- endunless -%}
                                                      {%- assign check_product_id = check_product_id | append: product.id -%}
                                                   {%- endif -%}
                                                {%- endfor-%}
                                             {%- endcapture -%}
                                             {%- if check_product_id != blank -%}
                                                   <div class="products row nt-products-holder nt-carousel {{block.settings.image_size}} {{block.settings.image_ratio}} position_{{block.settings.image_position}}{%- if settings_pr_img_size %} equal-nt{%- endif -%}" data-slick='{"slidesToShow": 1,"slidesToScroll": 1,"arrows":false,"dots": true,"rtl": {{ settings.use_rtl }}}'>
                                                      {{html_pr}}
                                                   </div>
                                             {%- endif -%}
                                       {%- endif -%}
                                 </div>
                                 {%- endif -%}
                              </div>
                           </div>
                        </div>
                     {%- endif -%}
                  </li>
                  {%- if label != blank and block.settings.color != blank -%}<style type="text/css">.menu-item-{{block.id}} >a>.menu-item-tag-{{label | handleize}}{ background-color: {{block.settings.color}} }.menu-item-{{block.id}} >a>.menu-item-tag-{{label | handleize}}:after{ color: {{block.settings.color}} }</style>{%- endif -%}
                {%- endif -%}
               {%- when 'banner' -%}
                {%- if block.settings.hidden_block == false -%}
                  {%- assign label = block.settings.label -%}
                  {%- assign icon = block.settings.icon -%}
                  {%- assign label_dow = label | downcase -%}
                  {%- assign link_list = block.settings.menu -%}
                  {%- assign block_image = block.settings.image -%}
                  <li id="menu-item-{{block.id}}" class="type_banner fullwidth menu-item menu-item-hover has-children nt-has-children  menu-item-{{block.id}}{%- if label_dow == 'sale' or label_dow == 'sale!' %} sale{%- endif -%}">
                     <a href="{{block.settings.button_link | default:'/'}}">{%- if icon != blank -%}<i class="fa fa-{{icon}}"></i>{%- endif -%}{{block.settings.title}}{%- if label != blank and block.settings.color != blank -%}<span class="menu-item-tag menu-item-tag-{{ label | handleize}}">{{label}}</span>{%- endif -%}</a>
                     {%- if link_list != blank -%}
                        <div class="nav_dropdown pa tl">
                           <div class="container">
                              <div class="row">
                                 <div class="{%- if block.settings.show_banner and block_image != blank -%}col-9{%- else -%}col-12{%- endif -%}">
                                    <div class="row">
                                       {%- assign child_index = 0 -%}
                                       {%- assign no_index = 0 -%}
                                       {%- for link in linklists[link_list].links -%}
                                          {%- assign child_list_handle = link.title | handleize -%}
                                          {%- if linklists[child_list_handle].links != blank or link.links != blank -%}
                                          {%- assign child_index = child_index | plus:1 -%}
                                          {%- else -%}
                                          {%- assign no_index = no_index | plus:1 -%}
                                          {%- endif -%}
                                       {%- endfor -%}
                                       {%- if child_index > 5 %}
                                         {%- assign link_list_size = 6 -%}
                                       {%- elsif no_index > 6 -%}
                                         {%- assign link_list_size = child_index |plus:2 -%}
                                       {%- elsif no_index > 0 -%}
                                         {%- assign link_list_size = child_index |plus:1 -%}
                                       {%- else -%}
                                         {%- assign link_list_size = child_index -%}
                                       {%- endif -%}
                                       {%- if link_list_size == 5 -%}{%- assign class = 'lg-15' -%}
                                       {%- elsif link_list_size > 5 -%}{%- assign class = '2' -%}
                                       {%- else -%}{%- assign class = 12 | divided_by:link_list_size -%}
                                       {%- endif -%}
                                       {%- assign index = 0 -%}
                                       {%- for link in linklists[link_list].links -%}
                                          {%- assign child_list_handle = link.title | handleize -%}
                                             {%- if linklists[child_list_handle].links != blank -%}
                                             {%- assign index = index |plus:1 -%}
                                             <div class="col-{{class}}">
                                                <ul class="sub-menu mega-menu">
                                                   <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                      <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                      <ul class="sub-column">
                                                         {%- for thirdlink in linklists[child_list_handle].links -%}
                                                            {%- assign icon_third  = thirdlink.title |  split: '[icon_' | last  | split: ']' | first -%}
                                                            {%- assign label_third  = thirdlink.title |  split: '[label_' | last  | split: ']' | first -%}
                                                            <li class="menu-item"><a href="{{ thirdlink.url }}">{%- if thirdlink.title contains '[icon_' -%}<i class="fa fa-{{icon_third}}"></i>{%- endif -%}{{ thirdlink.title | split: '[' | first }}{%- if thirdlink.title contains '[label_' -%}<span class="nt-menu-label nt-menu-label-{{label_third | handleize}}">{{label_third}}</span>{%- endif -%}</a></li>
                                                         {%- endfor -%}
                                                      </ul>
                                                   </li>
                                                </ul>
                                             </div>
                                             {%- elsif link.links != blank -%}
                                             {%- assign index = index |plus:1 -%}
                                             <div class="col-{{class}}">
                                                <ul class="sub-menu mega-menu">
                                                   <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                      <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                      <ul class="sub-column">
                                                         {%- for thirdlink in link.links -%}
                                                            {%- assign icon_third  = thirdlink.title |  split: '[icon_' | last  | split: ']' | first -%}
                                                            {%- assign label_third  = thirdlink.title |  split: '[label_' | last  | split: ']' | first -%}
                                                            <li class="menu-item"><a href="{{ thirdlink.url }}">{%- if thirdlink.title contains '[icon_' -%}<i class="fa fa-{{icon_third}}"></i>{%- endif -%}{{ thirdlink.title | split: '[' | first }}{%- if thirdlink.title contains '[label_' -%}<span class="nt-menu-label nt-menu-label-{{label_third | handleize}}">{{label_third}}</span>{%- endif -%}</a></li>
                                                         {%- endfor -%}
                                                      </ul>
                                                   </li>
                                                </ul>
                                             </div>
                                             {%- endif -%}
                                          {%- if index == 6 -%}{%- break -%}{%- endif -%}
                                       {%- endfor -%}
                                       {%- if child_index < 6 and no_index > 0 -%}
                                          <div class="col-{{class}}">
                                             <ul class="sub-menu mega-menu">
                                                {%- assign index_1 = 0 -%}
                                                {%- for link in linklists[link_list].links -%}
                                                   {%- assign child_list_handle = link.title | handleize -%}
                                                   {%- if linklists[child_list_handle].links == blank and link.links == blank -%}
                                                   <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                      <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                   </li>
                                                   {%- assign index_1 = index_1 |plus:1 -%}
                                                   {%- endif -%}
                                                   {% assign forindex = forloop.index -%}
                                                   {%- if index_1 == 6 -%}{%- break -%}{%- endif -%}
                                                {%- endfor -%}
                                             </ul>
                                          </div>
                                          {%- if no_index > 6 and child_index < 5 -%}
                                             <div class="col-{{class}}" forindex='{{forindex}}'>
                                                <ul class="sub-menu mega-menu">
                                                   {%- assign index_2 = 0 -%}
                                                   {%- for link in linklists[link_list].links offset:forindex -%}
                                                      {%- assign child_list_handle = link.title | handleize -%}
                                                      {%- if linklists[child_list_handle].links == blank and link.links == blank -%}
                                                      <li class="menu-item menu-item-has-children nt-has-children  sub-column-item">
                                                         <a href="{{ link.url }}">{{ link.title | split: '[' | first }}</a>
                                                      </li>
                                                      {%- assign index_2 = index_2 |plus:1 -%}
                                                      {%- endif -%}
                                                      {%- if index_2 == 6 -%}{%- break -%}{%- endif -%}
                                                   {%- endfor -%}
                                                </ul>
                                             </div>
                                          {%- endif -%}
                                       {%- endif -%}
